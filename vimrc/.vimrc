"leader 
let mapleader = "-"
let maplocalleader = "\\"

"map setting
inoremap <leader>u <esc>vawUea
nnoremap <leader>u vawUe
nnoremap <leader>d dd
nnoremap <leader>ev :vsplit $MYVIMRC<cr>
nnoremap <leader>sv :source $MYVIMRC<cr>


"tab -> 4 spaces
set tabstop=4
set shiftwidth=4
set expandtab

"turn on color
syntax on


